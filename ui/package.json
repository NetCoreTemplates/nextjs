{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dtos": "cd lib && x ts",
    "build": "next build",
    "build:local": "cross-env NODE_ENV=development MODE=local next build",
    "publish": "npm run build && dotnet publish ../api/MyApp/MyApp.csproj -c Release",
    "start": "next start",
    "typecheck": "tsc",
    "postinstall": "cd ../api/MyApp && dotnet run --AppTasks=migrate",
    "migrate": "cd ../api/MyApp && dotnet run MyApp.csproj --AppTasks=migrate",
    "revert:last": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:last",
    "revert:all": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:all",
    "rerun:last": "npm run revert:last && npm run migrate"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@next/mdx": "^15.1.0",
    "@servicestack/client": "^2.1.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.1",
    "swr": "^2.2.5",
    "typescript": "^5.7.2",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@iconify/react": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "cross-env": "^7.0.3",
    "tailwindcss": "^4.0.0"
  }
}
