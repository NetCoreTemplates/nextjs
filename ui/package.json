{
  "private": true,
  "scripts": {
    "dev": "next",
    "dtos": "cd lib && x ts",
    "build": "next build && next export -o ../api/MyApp/wwwroot",
    "build:local": "cross-env NODE_ENV=development MODE=local next build && next export -o ../api/MyApp/wwwroot",
    "publish": "npm run build && dotnet publish ../api/MyApp/MyApp.csproj -c Release",
    "start": "next start",
    "typecheck": "tsc",
    "postinstall": "cd ../api/MyApp && dotnet run --AppTasks=migrate",
    "migrate": "cd ../api/MyApp && dotnet run MyApp.csproj --AppTasks=migrate",
    "revert:last": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:last",
    "revert:all": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:all"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.1.3",
    "@next/mdx": "^12.2.5",
    "@servicestack/client": "^1.2.1",
    "classnames": "2.3.1",
    "date-fns": "2.29.2",
    "gray-matter": "4.0.3",
    "next": "^12.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "swr": "^1.3.0",
    "typescript": "^4.8.2",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@iconify/react": "^3.2.2",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8"
  }
}
